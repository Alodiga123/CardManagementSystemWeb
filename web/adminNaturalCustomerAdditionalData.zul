<?xml version="1.0" encoding="UTF-8"?>
<?taglib uri="/WEB-INF/tld/core.dsp.tld" prefix="c" ?>
<?page title="${c:l('sp.crud.profile.admin')}"?>
<?link rel="icon" tye="image/x-icon" href="/images/favicon.ico"?>

<zk xmlns:n="native">
    <window id="winAdminNaturalCustomerAdditionalData" width="100%" height="100%" apply="com.alodiga.cms.web.controllers.AdminNaturalCustomerAdditionalDataController" class="general-window">
        <style src="/css/general.css"/>
        <borderlayout width="100%" height="100%" class="content-transparent">
            <center border="none" flex="true" class="content-transparent" autoscroll="true">
                <div align="center" >
                    <hlayout >
                        <div></div>
                        <div width="1200px" align="left" height="780px">
                            <hlayout>
                                <div width="900px">
                                    <vlayout>
                                        <div height="5px" width="100%" style="background:#333;" align="left"></div>
                                        <div>
                                            <vlayout>
                                                <label value="${c:l('cms.crud.customer.additionalData.spouse')}" sclass="lblbold"/>
                                                <div height="5px" width="100%" style="background:#333;" align="left"></div>
                                                <grid fixedLayout="true">
                                                    <columns>
                                                        <column label="" width="290px"/>
                                                        <column label="" width="290px"/>
                                                        <column label="" width="290px"/>
                                                    </columns>
                                                    <rows>
                                                        <row>
                                                            <label value="${c:l('sp.common.country')}" sclass="lblbold"/>
                                                            <label value="${c:l('cms.common.identificationsTypes')}" sclass="lblbold"/>
                                                            <label value="${c:l('cms.crud.identificationNumber')}" sclass="lblbold"/>
                                                        </row>
                                                        <row>
                                                            <combobox id="cmbCountry" width="250px" mold="rounded" />
                                                            <combobox id="cmbDocumentsPersonType" width="250px" mold="rounded"/>
                                                            <textbox id="txtIdentificationNumber" value="" width="250px" maxlength="40"/>
                                                        </row>
                                                        <row>
                                                            <label value="${c:l('cms.common.fullName')}" sclass="lblbold"/>
                                                            <label value="${c:l('cms.common.fullLastName')}" sclass="lblbold"/>
                                                            <label value="${c:l('cms.common.phoneNumber')}" sclass="lblbold"/>
                                                        </row>
                                                        <row>
                                                            <textbox id="txtFirstNames" value="" width="250px" maxlength="40"/>
                                                            <textbox id="txtLastNames" value="" width="250px" maxlength="40"/>
                                                            <textbox id="txtPhone" value="" width="250px" maxlength="30"/>
                                                        </row>
                                                        <row>
                                                            <label value="${c:l('cms.common.email')}" sclass="lblbold"/>                                                            
                                                        </row>
                                                        <row>
                                                            <textbox id="txtEmail" value="" width="250px" maxlength="60"/>
                                                        </row>
                                                    </rows>
                                                </grid>
                                                <label value="${c:l('cms.crud.customer.additionalData.carData')}" sclass="lblbold"/>
                                                <div height="5px" width="100%" style="background:#333;" align="left"></div>
                                                <grid fixedLayout="true">
                                                    <columns>
                                                        <column label="" width="290px"/>
                                                        <column label="" width="290px"/>
                                                        <column label="" width="290px"/>
                                                    </columns>
                                                    <rows>
                                                        <row>
                                                            <label value="${c:l('cms.crud.customer.additionalData.carData.carBrand')}" sclass="lblbold"/>
                                                            <label value="${c:l('cms.crud.customer.additionalData.carData.carModel')}" sclass="lblbold"/>
                                                            <label value="${c:l('cms.crud.customer.additionalData.carData.carYear')}" sclass="lblbold"/>
                                                        </row>
                                                        <row>
                                                            <textbox id="txtCarBrand" value="" width="250px" maxlength="40"/>
                                                            <textbox id="txtCarModel" value="" width="250px" maxlength="40"/>
                                                            <textbox id="txtCarYear" value="" width="250px" maxlength="4"/>
                                                        </row>
                                                        <row>
                                                            <label value="${c:l('cms.crud.customer.additionalData.carData.carPlate')}" sclass="lblbold"/>
                                                        </row>
                                                        <row>
                                                            <textbox id="txtCarPlate" value="" width="250px" maxlength="20"/>
                                                        </row>
                                                    </rows>
                                                </grid>
                                                <label value="${c:l('cms.crud.customer.additionalData.financialInformation')}" sclass="lblbold"/>
                                                <div height="5px" width="100%" style="background:#333;" align="left"></div>
                                                <grid fixedLayout="true">
                                                    <columns>
                                                        <column label="" width="290px"/>
                                                        <column label="" width="290px"/>
                                                        <column label="" width="290px"/>
                                                    </columns>
                                                    <rows>
                                                        <row>
                                                            <label value="${c:l('cms.crud.customer.additionalData.financialInformation.income')}" sclass="lblbold"/>
                                                        </row>
                                                        <row>
                                                            <cell colspan="3">
                                                                <div height="0.5px" width="100%" style="background:#333;" align="left"></div>
                                                            </cell>
                                                        </row>
                                                        <row>
                                                            <label value="${c:l('cms.crud.customer.additionalData.financialInformation.salary')}" sclass="lblbold"/>
                                                            <label value="${c:l('cms.crud.customer.additionalData.financialInformation.profession')}" sclass="lblbold"/>
                                                            <label value="${c:l('cms.crud.customer.additionalData.financialInformation.bonuses')}" sclass="lblbold"/>
                                                        </row>
                                                        <row>
                                                            <textbox id="txtSalary" value="" width="250px" maxlength="20"/>
                                                            <textbox id="txtProfession" value="" width="250px" maxlength="20"/>
                                                            <textbox id="txtBonuses" value="" width="250px" maxlength="20"/>
                                                        </row>
                                                        <row>
                                                            <label value="${c:l('cms.crud.customer.additionalData.financialInformation.rentsIncome')}" sclass="lblbold"/>
                                                            <label value="${c:l('cms.crud.customer.additionalData.financialInformation.otherIncome')}" sclass="lblbold"/>
                                                            <label value="${c:l('cms.crud.customer.additionalData.financialInformation.totalIncome')}" sclass="lblbold"/>
                                                        </row>
                                                        <row>
                                                            <textbox id="txtRentIncome" value="" width="250px" maxlength="20"/>
                                                            <textbox id="txtOtherIncome" value="" width="250px" maxlength="20"/>
                                                            <textbox id="txtTotalIncome" value="" width="250px" maxlength="20"/>
                                                        </row>
                                                        <row>
                                                            <label value="${c:l('cms.crud.customer.additionalData.financialInformation.expenses')}" sclass="lblbold"/>
                                                        </row>
                                                        <row>
                                                            <cell colspan="3">
                                                                <div height="0.5px" width="100%" style="background:#333;" align="left"></div>
                                                            </cell>
                                                        </row>
                                                        <row> 
                                                            <label value="${c:l('cms.crud.customer.additionalData.financialInformation.housingExpenses')}" sclass="lblbold"/>
                                                            <label value="${c:l('cms.crud.customer.additionalData.financialInformation.monthlyRentMortgage')}" sclass="lblbold"/>
                                                            <label value="${c:l('cms.crud.customer.additionalData.financialInformation.monthlyPaymentCreditCard')}" sclass="lblbold"/>
                                                        </row>
                                                        <row>
                                                            <textbox id="txtHousingExpenses" value="" width="250px" maxlength="20"/>
                                                            <textbox id="txtMonthlyRentMortgage" value="" width="250px" maxlength="20"/>
                                                            <textbox id="txtMonthlyPaymentCreditCard" value="" width="250px" maxlength="20"/>
                                                        </row>
                                                        <row>
                                                            <label value="${c:l('cms.crud.customer.additionalData.financialInformation.monthlyPaymentOtherCredit')}" sclass="lblbold"/>
                                                            <label value="${c:l('cms.crud.customer.additionalData.financialInformation.educationExpenses')}" sclass="lblbold"/>
                                                            <label value="${c:l('cms.crud.customer.additionalData.financialInformation.totalExpenses')}" sclass="lblbold"/>
                                                        </row>
                                                        <row>
                                                            <textbox id="txtMonthlyPaymentOtherCredit" value="" width="250px" maxlength="20"/>
                                                            <textbox id="txtEducationExpenses" value="" width="250px" maxlength="20"/>
                                                            <textbox id="txtTotalExpenses" value="" width="250px" maxlength="20"/>
                                                        </row>
                                                    </rows>
                                                </grid>
                                            </vlayout>
                                        </div>
                                        <div id="divInfo" height="20px" width="100%" style="background:#B6E59E;" align="left" visible="false"/>
                                        <!--Buttons-->
                                        <div align="right">
                                            <button  id="btnSave" width="150px" label="${c:l('sp.common.actions.save')}" class="open orange" />
                                            <button  id="btnBack" width="150px" label="${c:l('sp.common.actions.back')}" class="open gray" onClick="Executions.sendRedirect(&quot;listCustomer.zul&quot;)"/>
                                        </div>
                                    </vlayout>
                                </div>
                                <div></div>
                            </hlayout>
                        </div>
                        <div></div>
                    </hlayout>
                </div>
            </center>
        </borderlayout>
    </window>
</zk>